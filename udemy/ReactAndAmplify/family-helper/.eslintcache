ons';\n\nAmplify.configure(awsConfig);\n\nconst actions = {\n  TITLE_CHANGE: 'TITLE_CHANGE',\n  DESCRIPTION_CHANGE: 'DESCRIPTION_CHANGE'\n}\n\nconst intialState = {\n  title: '',\n  description: ''\n}\nfunction listReducer(state = intialState, action) {\n  switch (action.type) {\n    case actions.DESCRIPTION_CHANGE:\n      return {...state, description: action.value}\n    case actions.TITLE_CHANGE:\n      return {...state, title: action.value}\n    default:\n      console.log('Default action for', action);\n      return state\n  }\n}\n\nfunction App() {\n  const [state, dispatch] = useReducer(listReducer, intialState);\n\n  const [lists, setLists] = useState([]);\n  const [newList, setNewList] = useState('');\n  const [isModalOpen, setISModalOpen] = useState(false);\n  \n  async function fetchList() {\n    const { data } = await API.graphql(graphqlOperation(listLists));\n    setLists(data.listLists.items);\n    console.log(data);\n  }\n  useEffect(() => {\n     fetchList();\n  }, []);\n\n  useEffect(() => {\n    if (newList !== '') {\n      setLists([newList, ...lists]);\n    }\n  }, [newList]);\n\n  function addToList({data}) {\n    setNewList(data.onCreateList);\n  }\n\n  useEffect(() => {\n    let subscription = API\n    .graphql(graphqlOperation(onCreateList))\n    .subscribe({\n      next: ({provider, value}) => addToList(value)\n    });\n  }, []);\n\n  function toggleModal(shouldOpen) {\n   setISModalOpen(shouldOpen);\n  }\n\n  async function saveList() {\n    const { title, description } = state\n    const result = await API.graphql(graphqlOperation(createList, { input: { title, description } })); \n    toggleModal(false);\n    console.log('Saved data with result: ', result);\n  }\n\n  return (\n    <AmplifyAuthenticator>\n      <Container style={{height: '100vh'}}>\n        <AmplifySignOut />\n        <Button className=\"floatingButton\" onClick={() =>toggleModal(true)}>\n          <Icon name=\"plus\" className=\"floatingButton_icon\"/>\n        </Button>\n        <div className=\"App\">\n          <MainHeder />\n          <ul>\n            <Lists lists={lists} />\n          </ul>\n        </div>\n      </Container>\n      <Modal open={isModalOpen} dimmer=\"blurring\">\n        <Modal.Header>Create your list</Modal.Header>\n        <Modal.Content>\n          <Form>\n            <Form.Input \n              error={true ? false : {content: \"please add a name to your list\"}}\n              label=\"Title\" \n              placeholder=\"My pretty list\"\n              value={state.title}\n              onChange={(e) => dispatch({ type: actions.TITLE_CHANGE, value: e.target.value })}\n              ></Form.Input>\n            <Form.TextArea \n              label=\"Description\"\n              placeholder=\"Things that my pretty list is about\"\n              value={state.description}\n              onChange={(e) => dispatch({ type: actions.DESCRIPTION_CHANGE, value: e.target.value })}\n              ></Form.TextArea>\n          </Form>\n        </Modal.Content>\n        <Modal.Actions>\n          <Button negative onClick={() =>toggleModal(false)}>Cancel</Button>\n          <Button positive onClick={saveList}>Save</Button>\n        </Modal.Actions>\n      </Modal>\n    </AmplifyAuthenticator>\n  );\n}\n\nexport default App;\n","/Users/romelcampbell/GitHub/Repos/GraphQL/udemy/ReactAndAmplify/family-helper/src/index.js",[],["50","51"],"/Users/romelcampbell/GitHub/Repos/GraphQL/udemy/ReactAndAmplify/family-helper/src/aws-exports.js",[],"/Users/romelcampbell/GitHub/Repos/GraphQL/udemy/ReactAndAmplify/family-helper/src/graphql/queries.js",[],"/Users/romelcampbell/GitHub/Repos/GraphQL/udemy/ReactAndAmplify/family-helper/src/components/headers/MainHeader.js",[],"/Users/romelcampbell/GitHub/Repos/GraphQL/udemy/ReactAndAmplify/family-helper/src/components/Lists/Lists.js",[],["52","53"],"/Users/romelcampbell/GitHub/Repos/GraphQL/udemy/ReactAndAmplify/family-helper/src/components/Lists/List.js",[],"/Users/romelcampbell/GitHub/Repos/Gra